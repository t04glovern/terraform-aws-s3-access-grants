{
    "Iam": {
        "ActorPolicy": {
            "Version": "2012-10-17",
            "Statement": [
                {
                    "Effect": "Allow",
                    "Principal": {
                        "AWS": "arn:aws:iam::123456789012:root"
                    },
                    "Action": "sso-oauth:CreateTokenWithIAM",
                    "Resource": "*"
                }
            ]
        }
    }
}